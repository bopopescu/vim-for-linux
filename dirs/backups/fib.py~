"""
    斐波那契数
"""
def fib(num):
    a, b = 0, 1
    for i in range(0, (num-2)):
        print(a)
        a, b = b, a+b



def fib1(n):
    if n < 2:
        return n
    else:
        return fib1(n-1)+fib1(n-2)

a = lambda n : n if n > 2 else a(n-1) + a(n-2) 


if __name__ == "__main__":
    for i in range(10):
        print(a(i))
