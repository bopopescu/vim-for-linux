  function setDate(key,value){
    data[key]=value
  }

  function setALLCurrentDate(){
    var current_date_format=getCurrentDateFormat()
    data={
        "freeze-user-list":current_date_format,
        "deleted_post":current_date_format,
        "great_post":current_date_format,
