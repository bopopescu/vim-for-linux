# -*- coding: utf-8 -*-
# Created Time: 2018-08-03 09:23:36




def bubble_sort(lst):
    for i in range(len(lst)):
        for c in range(i, len(lst)):
            if lst[i] < lst[c]:
                lst[i], lst[c] = lst[c], lst[i]
    return lst



if __name__ == "__main__":
    lst = [2, 5, 1, 3, 4]
    c = bubble_sort(lst)
    print(c)
