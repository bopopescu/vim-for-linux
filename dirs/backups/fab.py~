# 一个数列 第三项 为 前两项 之和

def fib(n):
    a, b = 0, 1
    for i in range(n):
        c = a+b
        print(c)
        a = b
        b = c



def fib2(n):
    if n < 2:
        return n
    return fib2(n-1) + fib2(n-2)


for i in range(10):
    print i


def digui(n):
    if n > 0:
        print n
        return
    digui(n-1)





if __name__ == "__main__":
    fib(10)
